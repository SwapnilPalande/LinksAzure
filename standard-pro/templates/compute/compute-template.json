{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "type": "string",
      "allowedValues": [
        "ase",
        "ae",
        "ukw",
        "uks",
        "usw",
        "use"
      ]
    },
    "deploymentregion": {
      "type": "string"
    },
    "customerid": {
      "type": "string"
    },
    "environment": {
      "type": "string",
      "allowedValues": [
        "prod",
        "dev"
      ]
    },
    "tier": {
      "type": "string",
      "allowedValues": [
        "standard",
        "premium"
      ]
    },
    "function": {
      "type": "string",
      "allowedValues": [
        "network",
        "compute",
        "backup",
        "data"
      ]
    }
  },
  "variables": {},
  "resources": [],
  "outputs": {}
  }